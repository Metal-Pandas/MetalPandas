      <ImageView fx:id="ratingPic4" fitHeight="53.0" fitWidth="50.0" layoutX="324.0" layoutY="25.0" pickOnBounds="true" preserveRatio="true">
        <Image url="@rating.png" />
      </ImageView>
      <ImageView fx:id="ratingPic5" fitHeight="53.0" fitWidth="50.0" layoutX="424.0" layoutY="25.0" pickOnBounds="true" preserveRatio="true">
        <Image url="@rating.png" />
      </ImageView>
    </Pane>
    <Label fx:id="ratingLabel1" layoutX="102.0" layoutY="126.0" text="1">
