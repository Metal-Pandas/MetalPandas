    <Label fx:id="ratingLabel5" layoutX="497.0" layoutY="126.0" text="5">
      <font>
        <Font name="Monospaced Bold" size="12.0" />
      </font></Label>
  </Pane>
  <stylesheets>
    <URL value="@lightTheme.css" />
  </stylesheets>
