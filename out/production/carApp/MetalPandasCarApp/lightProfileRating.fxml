        <Font name="Monospaced Regular" size="36.0" />
      </font>
    </Label>
    <Button fx:id="confirmRating" layoutX="252.0" layoutY="256.0" mnemonicParsing="false" onAction="#handleRateAction" text="Confirm Rating" />
    <Pane layoutX="56.0" layoutY="143.0" prefHeight="101.0" prefWidth="502.0">
      <Circle fx:id="rating1" accessibleRole="BUTTON" fill="TRANSPARENT" layoutX="49.0" layoutY="51.0" onMouseMoved="#handleRating1Action" radius="45.0" stroke="BLACK" strokeType="INSIDE" />
      <Circle fx:id="rating2" accessibleRole="BUTTON" fill="TRANSPARENT" layoutX="149.0" layoutY="51.0" onMouseMoved="#handleRating2Action" radius="45.0" stroke="BLACK" strokeType="INSIDE" />
