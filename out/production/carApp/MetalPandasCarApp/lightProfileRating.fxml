      <ImageView fx:id="ratingPic1" fitHeight="53.0" fitWidth="50.0" layoutX="24.0" layoutY="24.0" pickOnBounds="true" preserveRatio="true">
        <Image url="@rating.png" />
      </ImageView>
      <ImageView fx:id="ratingPic2" fitHeight="53.0" fitWidth="50.0" layoutX="124.0" layoutY="24.0" pickOnBounds="true" preserveRatio="true">
        <Image url="@rating.png" />
